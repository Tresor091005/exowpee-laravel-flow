{
    "name": "exowpee/laravel-flow",
    "description": "A Laravel package for managing contextual flows with plugin isolation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Happiness LAHAN",
            "email": "happinesstresor3@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/events": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.5",
        "pestphp/pest": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Exowpee\\LaravelFlow\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Exowpee\\LaravelFlow\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Exowpee\\LaravelFlow\\FlowServiceProvider"
            ],
            "aliases": {
                "Flow": "Exowpee\\LaravelFlow\\Facades\\Flow"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}